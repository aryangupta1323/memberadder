<%- include("partials/header"); -%>
  <h1>Members List</h1>
  <div class="tbl-header">
      <table cellpadding="0" cellspacing="0" border="0">
        <thead>
          <tr>
            <th>S.No</th>
            <th>Name</th>
            <th>Contact</th>
            <th>Email</th>
            <th>Edit</th>
          </tr>
        </thead>
      </table>
    </div>
    <div class="tbl-content">
      <table cellpadding="0" cellspacing="0" border="0">
        <tbody>
      <% for(let i=0;i<usersArray.length;i++){ %>
        <tr>
          <td><%= i+1%></td>
          <td><%= usersArray[i].name%></td>
          <td><%= usersArray[i].email%></td>
          <td><%= usersArray[i].contact%></td>
          <td><a href="/edit/<%= usersArray[i]._id %>"><button type="button" name="button" class="btn btn-primary">Edit</button></a></td>
        </tr>
      <%} %>
    </tbody>
   </table>
 </div>
  </body>
</html>
